                effect:"fold",
                interTime:3500,
                delayTime:500,
                autoPlay:true,
                autoPage:true,
                trigger:"click"
            });
            $(".btn_top").click(function(){
                $("html,body").animate({scrollTop:'0'},400);return false;
            });
        });
    </script>
